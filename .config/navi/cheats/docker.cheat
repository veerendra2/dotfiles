% docker

# Launch it-tools docker container
docker run -d --name it-tools --restart unless-stopped -p 8006:80 corentinth/it-tools:latest

# Show complete output during build
export BUILDKIT_PROGRESS=plain

# Show full output
docker build --progress=plain --no-cache -t tag .

# Docker compose config
docker compose config --no-normalize --no-interpolate --output compose.production.yml

# Add current user to docker group
sudo usermod -aG docker $USER